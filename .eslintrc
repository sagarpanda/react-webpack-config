{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": truen,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "quotes": [2, "single"],
    "indent": [2, 2],
    "no-extra-parens": [2],
    "no-template-curly-in-string": [2],
    "valid-jsdoc": [2],
    "dot-notation": [2],
    "eqeqeq": [2],
    "no-alert": [2],
    "no-caller": [2],
    "no-else-return": [2],
    "no-empty-function": [2],
    "no-eq-null": [2],
    "no-eval": [2],
    "no-floating-decimal": [2],
    "no-implicit-coercion": [2],
    "no-implied-eval": [2],
    "no-loop-func": [2],
    "no-multi-spaces": [2],
    "no-multi-str": [2],
    "no-new-wrappers": [2],
    "no-param-reassign": [2],
    "no-proto": [2],
    "no-return-assign": [2],
    "no-script-url": [2],
    "no-void": [2],
    "no-warning-comments": [2],
    "no-with": [2],
    "radix": [2],
    "space-before-blocks": [2],
    "semi-spacing": [2, {"before": false, "after": true}],
    "no-trailing-spaces": "error",
    "spaced-comment": [2, "always"],
    "no-var": [2],
    "no-useless-constructor": [2],
    "no-useless-computed-key": [2],
    "no-duplicate-imports": [2],
    "no-confusing-arrow": [2, {"allowParens": false}],
    "arrow-spacing": [2],
    "switch-colon-spacing": [2],
    "space-unary-ops": [2],
    "space-infix-ops": [2],
    "sort-keys": [2],
    "semi-style": [2],
    "operator-linebreak": [2, "after"],
    "no-tabs": [2],
    "lines-between-class-members": [2]
  },
  "globals": {
    "document": true,
    "__dirname": true,
    "module": true,
    "test": true,
    "expect": true,
    "window": true
  }
}
